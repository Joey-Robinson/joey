{
  "name": "joey",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass src/sass/main.scss css/style.css -w",
    "compile:sass": "node-sass src/sass/main.scss css/style.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css/style.prefix.css",
    "start": "webpack-dev-server --config ./webpack.config.js src/sass/main.scss -w",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "compile:sass concat:css prefix:css compress:css",
    "build:dev": "webpack",
    "build:prod": "webpack -p --env production"
  },
  "keywords": [],
  "author": "Joey Robinson <joeyrobinsondev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.23.1",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "jest": "^23.4.1",
    "postcss": "^6.0.22",
    "postcss-cli": "^5.0.1",
    "react-hot-loader": "^3.1.3",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.10",
    "node-sass": "^4.9.2",
    "normalize.css": "^8.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.2"
  }
}
